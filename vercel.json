{
    "version": 2,
    "builds": [
      
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      
      {
        "source": "/api/(.*)",
        "destination": "api/app.py"
      },
      {
        "source": "/(.*)",
        "destination": "public/$1"
      }
        ]
  }