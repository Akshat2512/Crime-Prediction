{
    "version": 2,
    "builds": [
      
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "Data/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "static/**/*",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      
      
      {
        "source": "/(.*)",
        "destination": "public/index.html"
      },
      {
        "source": "/(.*)",
        "destination": "api/app.py"
      }
        ]
  }