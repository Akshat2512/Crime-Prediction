{
    "version": 2,
    "builds": [
      
      {
        "src": "api/app.py",
        "use": "@vercel/python",
        "config": {
            "installCommand": "pip install -r requirements.txt"
          }
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      
      
      {
        "source": "/(.*)",
        "destination": "public/$1"
      },
      {
        "source": "/(.*)",
        "destination": "api/app.py"
      }
        ]
  }