{
    "version": 2,
    "builds": [
      {
        "src": "public/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api/app.py"
      },
      {
        "source": "/(.*)",
        "destination": "/public/index.html"
      },
      {
        "source": "/(.*)",
        "destination": "/Module/**/*.py"
      }
    ]
  }